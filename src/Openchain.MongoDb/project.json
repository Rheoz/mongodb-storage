{
  "version": "0.2.0",
  "description": "MongoDb storage module for Openchain Server",
  "authors": [ "Frédéric Luce" ],

  "buildOptions": {
    "allowUnsafe": false,
    "warningsAsErrors": true
  },

  "packOptions": {
    "projectUrl": "https://github.com/openchain/mongodb-storage",
    "licenseUrl": "https://www.apache.org/licenses/LICENSE-2.0",
    "iconUrl": "https://www.openchain.org/package.png"
  },

  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "MongoDB.Driver": "2.3.0",
        "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
        "Openchain.Abstractions": "0.7.0",
        "Openchain.Infrastructure": "0.7.0"
      }
    }
  }
}
